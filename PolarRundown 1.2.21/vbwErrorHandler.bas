' gets the whole file in a string
Private Function LoadFile(ByVal sFileName As String) As String
220    On Error GoTo erreur
221        Dim nFile As Integer, sText As String
222        nFile = FreeFile
           'Open sFileName For Input As nFile ' Don't do this!!!
223        If Not FileExist(sFileName) Then
224             Exit Function
225        End If
           ' Let others read but not write
226        Open sFileName For Binary As nFile
227        sText = String$(LOF(nFile), 0)
228        Get nFile, 1, sText
229        Close nFile
230        LoadFile = sText
231    erreur:
End Function

' function FileExist not requiring a dir$ call
Private Function FileExist(ByVal tFile As String) As Boolean
232        On Error Resume Next
233        Call VBA.FileLen(tFile)
234        FileExist = (Err = 0)
End Function

' This disables all active timers because they might raise events during error handling time, '
' which would lead to unexpected results.                                                     '
' If you have other "outside" sources of events in your project, such as custom timers or     '
' subclassing code, this is the place to disable them temporarily.                            '
Private Sub TurnOffTimers()
235        Dim f As Form
236        Dim c As Control

237        On Error Resume Next
238        vbwTurnOffTimersString = Chr(0) ' Chr(0) = delimiter
239        For Each f In Forms
240            For Each c In f.Controls
241                If TypeName(c) = "Timer" Then
242                    If c.Enabled = True Then
                           ' remember its state by way of its object address
243                        vbwTurnOffTimersString = vbwTurnOffTimersString & ObjPtr(c) & Chr(0)
244                        c.Enabled = False
245                    End If
246                End If
247            Next c
248        Next f
End Sub

Private Sub TurnOnTimers()
249        Dim f As Form
250        Dim c As Control

251        On Error Resume Next
252        For Each f In Forms
253            For Each c In f.Controls
254                If TypeName(c) = "Timer" Then
255                    If InStr(vbwTurnOffTimersString, Chr(0) & ObjPtr(c) & Chr(0)) > 0 Then
256                        If ObjPtr(c) > 0 Then
257                            c.Enabled = True
258                        End If
259                    End If
260                End If
261            Next c
262        Next f
End Sub


